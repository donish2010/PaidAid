<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uw.tacoma.edu.paidaid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/paidaidlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/paidaidlogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.LoginActivity"
            android:logo="@mipmap/ic_launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- <activity android:name=".view.LoginActivity"></activity> -->
        <activity android:name=".view.AccountSettingsActivity" />
        <activity android:name=".view.HomeButtonActivity" />
        <activity android:name=".view.AddRequestButtonActivity" />
        <activity android:name=".view.MessagesButtonActivity" />
        <activity android:name=".view.RequestsButtonActivity"/>
        <activity android:name=".view.HomeActivity"></activity>
    </application>

</manifest>